<table border="1" cellspacing = "0">

  <tr>
    <td>
      Id
    </td>
    <td>
      email
    </td>
     <td>
      password
    </td>
     <td>
      name
    </td>
     <td>
      surname
    </td>
     <td>
      created_at
    </td>
     <td>
      updated_at
    </td>
  </tr>

<% @users.each do |user| %>

  <tr>
    <td>
      <%= user.id %>
    </td>
    <td>
      <%= link_to(user.email, user_path(user)) %>
    </td>
     <td>
      <%= user.password %>
    </td>
     <td>
      <%= user.name %>
    </td>
     <td>
      <%= user.surname %>
    </td>
     <td>
      <%= user.created_at %>
    </td>
     <td>
      <%= user.updated_at %>
    </td>
  </tr> 

<% end %> 

</table>





